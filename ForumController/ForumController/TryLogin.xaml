<Window x:Class="ForumController.TryLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TryLogin" Height="300" Width="300" ResizeMode="NoResize" Topmost="False">
    <Grid>
        <Label Content="Login" Height="28" HorizontalAlignment="Left" Margin="30,30,0,0" Name="loginLabel" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="30,64,0,0" Name="loginTextBox" VerticalAlignment="Top" Width="120" />
        <Label Content="Password" Height="28" HorizontalAlignment="Left" Margin="30,93,0,0" Name="passLabel" VerticalAlignment="Top" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="30,127,0,0" Name="passTextBox" VerticalAlignment="Top" Width="120" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="75,165,0,0" Name="okButton" VerticalAlignment="Top" Width="75" Click="okButton_Click" />
        <Label Content="" Height="28" HorizontalAlignment="Left" Margin="52,203,0,0" Name="stateLogLabel" VerticalAlignment="Top" Width="142" />
    </Grid>
</Window>
